<UserControl x:Class="Project"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:hostManagement"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="650" Width="650">
    <Grid>
        <WrapPanel Width="650" UseLayoutRounding="False">
            <Label x:Name="projectName" Content="Label"/>
            <Label x:Name="projectIp"  Content="Label"/>
            <Button Content="Edit" Click="editProject"/>
            <Button Content="Open Web" Click="openInWeb"/>
        </WrapPanel>

    </Grid>
</UserControl>
